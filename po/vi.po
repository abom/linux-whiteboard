#: ../src/gtk-gui.cpp:163
msgid "Configurations successfully loaded.\n"
msgstr "Đã nạp xong các thiết lập.\n"

#: ../src/gtk-gui.cpp:164
msgid "Failed to load configuration file.\n"
msgstr "Không nạp được các thiết lập cũ.\n"

#. NOTE: This is mainly to force the GUI to update itself
#. Probably because if it performs too fast, GTK wouldn't
#. have a chance to update the GUI.
#. NOTE: Better to call some update functions.
#: ../src/gtk-gui.cpp:205
msgid "Preparing to connect...\n"
msgstr "Chuẩn bị kết nối...\n"

#: ../src/gtk-gui.cpp:226
msgid "Successfully disconnected all Wiimotes.\n"
msgstr "Ngắt kết nối thành công.\n"

#: ../src/gtk-gui.cpp:227
msgid ""
"There was an error disconnecting the Wiimotes. Hell's broken loose!!1.\n"
msgstr ""
"Không ngắt kết nối với Wiimote được!. Bạn nên khởi động lại chương trình.\n"

#: ../src/gtk-gui.cpp:250
msgid "Calibration succeeded.\n"
msgstr "Đã chỉnh xong Wiimote.\n"

#: ../src/gtk-gui.cpp:251
msgid "User escaped or there was an error during calibration.\n"
msgstr "Người dùng đã bấm nút thoát hoặc có lỗi khi chỉnh Wiimote.\n"

#: ../src/gtk-gui.cpp:304
#, c-format
msgid "Connecting to Wiimote #%d... "
msgstr "Đang kết nối tới Wiimote số %d... "

#: ../src/gtk-gui.cpp:318
msgid "Succeeded!.\n"
msgstr "Kết nối thành công!.\n"

#: ../src/gtk-gui.cpp:318
msgid "Failed.\n"
